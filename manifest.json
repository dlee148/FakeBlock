{
  "manifest_version": 2,
  "name": "FakeBlock",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "http://www.facebook.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
