{
  "manifest_version": 2,
  "name": "FakeBlock",
  "version": "0.1",
  "browser_action": {
    "default_title": "FakeBlock",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "jquery-3.2.0.js",
        "content.js"
      ]
    }
  ]
}
